<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCatsDay1</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
    <h1> Hello, <%= current_user.user_name %> </h1>
    <%= button_to "Logout", session_url, method: :delete %>

  <% else %>
    <h3> You are not currentlly logged in.
        Please <%= link_to "Log in", new_session_url %>
    </h3>
  <% end %>
  <%= yield %>

</body>
</html>
